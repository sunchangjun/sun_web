<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sun.web.dao.IptvConfigDao" >

	<select id="findByKey" resultType="com.sun.web.domain.IptvConfig">
		SELECT * FROM iptv_config c WHERE c.key=#{key}
	 </select>


    <select id="findAll" resultType="com.sun.web.domain.IptvConfig">
        SELECT * FROM iptv_config
    </select>
	 
	<select id="findByKeys" resultType="com.sun.web.domain.IptvConfig">
		SELECT * FROM iptv_config c WHERE 1 = 1
		<if test="keys != null">
			and c.key in
			<foreach item="item" index="index" collection="keys" open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
	 </select>


</mapper>